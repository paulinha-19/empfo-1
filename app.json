      "package": "com.paulinha19.empfo1",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD0xFYq39zmE-wAV3xmMMibLAS_hlWast4"
        }
      },
      "web": {
        "favicon": "./assets/favicon.png"
      },
      "extra": {
